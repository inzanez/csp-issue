{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-mkdir",
    "fs:allow-exists",
    "fs:allow-app-write",
    "fs:allow-app-write-recursive",
    {
      "identifier": "fs:scope",
      "allow": [
        { "path": "$APPCONFIG/*" },
        { "path" : "$CONFIG/*"}
      ]
    },
    "shell:allow-open",
    "dialog:allow-open",
    "dialog:allow-save",
    "dialog:allow-ask",
    "dialog:allow-confirm",
    "dialog:default",
    "fs:default",
    "shell:default"
  ]
}